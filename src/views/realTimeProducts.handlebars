    <li>Apellido: {{this.apellido}}</li>
    <li>Edad: {{this.edad}}</li>
    <li>ID: {{this.id}}</li>
  {{/each}}
</ul>

<script src="./socket.io/socket.io.js"></script>
<script src="/js/scripts.js"></script>